<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Thunderstruck.UI.Views.Project.AboutPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="About Thunderstruck"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Label x:Name="lblUser"/>
            <Label x:Name="lblAttribution" Text="Uicons by https://www.flaticon.com/uicons - Flaticon "/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>